generator client {
  provider = "prisma-client-js"
  output   = "./generated/test-client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model User {
  id        String   @id
  email     String   @unique
  name      String
  status    String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}
