generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/@prisma/user-client"
}

datasource db {
  provider = "sqlite"
  url      = env("USER_DATABASE_URL")
}

model User {
  id        String   @id
  email     String   @unique
  name      String
  status    String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}
