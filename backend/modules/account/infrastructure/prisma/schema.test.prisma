generator client {
  provider = "prisma-client-js"
  output   = "./generated/test-client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model Account {
  id        String   @id
  balance   Float
  status    String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("accounts")
}
